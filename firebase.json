{
  "hosting": [
    {
      "target": "util",
      "public": "build/web"
    },
    {
      "target": "promotion",
      "public": "nodejs/promotion"
    }
  ],
  "functions": [
    {
      "source": "functions/discord_auth",
      "codebase": "discord_auth",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log"
      ],
      "predeploy": [
        "npm --prefix \"$RESOURCE_DIR\" run build"
      ]
    },
    {
      "source": "functions/visit_counter",
      "codebase": "visit_counter",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log"
      ],
      "predeploy": [
        "npm --prefix \"$RESOURCE_DIR\" run build"
      ]
    }
  ]
}